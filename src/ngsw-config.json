{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html"
        ],
        "versionedFiles": [
          "/*.bundle.css",
          "/*.bundle.js",
          "/*.chunk.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "Sources",
      "urls": [
        "/sources"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 30,
        "maxAge": "7d",
        "timeout": "5s"
      }
    },
    {
      "name": "Top headlines",
      "urls": [
        "/top-headlines"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 60,
        "maxAge": "1d",
        "timeout": "5s"
      }
    },
    {
      "name": "Everything",
      "urls": [
        "/everything"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 30,
        "maxAge": "1h",
        "timeout": "5s"
      }
    }
  ]
}
